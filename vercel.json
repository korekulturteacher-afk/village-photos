{
  "functions": {
    "app/api/thumbnail/[fileId]/route.ts": {
      "maxDuration": 60
    },
    "app/api/image/[fileId]/route.ts": {
      "maxDuration": 60
    },
    "app/api/download-photo/[requestId]/[photoId]/route.ts": {
      "maxDuration": 60
    },
    "app/api/download/[requestId]/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
